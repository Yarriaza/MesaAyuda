    private void botonGuardarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_botonGuardarActionPerformed

        ValidaForm objValidaForm = new ValidaForm();
        ClsTicket objTicket = new ClsTicket();
        ClsDatosTicket objDts = new ClsDatosTicket(); 

        objDts.setRutCliente(cajaRut.getText());
        objDts.setNombreCliente(cajaNombre.getText());
        objDts.setTelefonoCliente(cajaTelefono.getText());
        objDts.setEmailCliente(cajaCorreo.getText());
        objDts.setDireccionCliente(cajaDireccion.getText());
        objDts.setCodigoEjecutivo(comboEjecutivo.getSelectedIndex());
        objDts.setCodigoArea(comboArea.getSelectedIndex());
        objDts.setCodigoTipo(comboTipo.getSelectedIndex());
        objDts.setCodigoCriticidad(comboCriticidad.getSelectedIndex());
        objDts.setDetalleServicio(cajaServicio.getText());
        objDts.setDetalleProblema(cajaProblema.getText());

        if (objValidaForm.validaIngresoTicket(objDts)) {
            int numticket;
            numticket= objTicket.InsertarTicket(objDts);
            if(numticket > 0){           
                JOptionPane.showMessageDialog(null, "Ticket guardado Nro. Ticked: " + numticket);
                
                //visualizacion
                  Previsualizacion objPre = new Previsualizacion();
                  //objDts.setCodigoTicket(numticket);
                  objPre.NumTicket = numticket;
                  objPre.setVisible(true);
                            
            }else{
                JOptionPane.showMessageDialog(null,"No se pudo insertar ticket");
            }
            
        } else {
            JOptionPane.showMessageDialog(null, "Favor ingresar todos los datos del cliente.");
        }


    }//GEN-LAST:event_botonGuardarActionPerformed


    private void cajaNombreKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_cajaNombreKeyTyped
        char validar = evt.getKeyChar();
        
        if(Character.isDigit(validar)){
            getToolkit().beep();
            evt.consume();
            
//            JOptionPane.showMessageDialog(rootPane, "Debe ingresar solo letras");
            
        }
    }//GEN-LAST:event_cajaNombreKeyTyped

    private void cajaTelefonoKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_cajaTelefonoKeyTyped
        char validar = evt.getKeyChar();
        
        if(Character.isLetter(validar)){
            getToolkit().beep();
            evt.consume();
            
//            JOptionPane.showMessageDialog(rootPane, "Debe ingresar solo numeros");
            
        }
    }//GEN-LAST:event_cajaTelefonoKeyTyped

    private void cajaRutFocusLost(java.awt.event.FocusEvent evt) {//GEN-FIRST:event_cajaRutFocusLost

        ClsClientes objcliente = new ClsClientes();
        ClsDatosTicket objDts = new ClsDatosTicket();
       
        objDts.setRutCliente(cajaRut.getText());

        if (!cajaRut.getText().trim().equals("")) {

            ResultSet DatosCliente = objcliente.ConsultarRutCliente(objDts);

            if (DatosCliente == null) {
                cajaNombre.setText("");
                cajaTelefono.setText("");
                cajaCorreo.setText("");
                cajaDireccion.setText("");
                JOptionPane.showMessageDialog(null, "Cliente no existe.");

            } else {
                try {

                    cajaNombre.setText(DatosCliente.getString("NOMBRECLIENTE"));
                    cajaTelefono.setText(DatosCliente.getString("TELEFONOCLIENTE"));
                    cajaCorreo.setText(DatosCliente.getString("EMAILCLIENTE"));
                    cajaDireccion.setText(DatosCliente.getString("DIRECCIONCLIENTE"));

                } catch (SQLException ex) {
                    JOptionPane.showMessageDialog(null, "Problemas al traer la data del cliente.");
                }

            }
        } else {
            cajaNombre.setText("");
            cajaTelefono.setText("");
            cajaCorreo.setText("");
            cajaDireccion.setText("");
        }
    }//GEN-LAST:event_cajaRutFocusLost

